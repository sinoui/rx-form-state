{
  "name": "@sinoui/rx-form-state",
  "description": "表单状态管理库",
  "version": "1.0.3",
  "license": "MIT",
  "homepage": "https://sinoui.github.io/rx-form-state",
  "bugs": {
    "url": "https://github.com/sinoui/rx-form-state/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sinoui/rx-form-state.git"
  },
  "main": "index.js",
  "module": "ems/index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "ts-lib-tools test",
    "build": "ts-lib-tools build --simple",
    "format": "ts-lib-tools format",
    "lint": "ts-lib-tools lint",
    "release": "yarn build && yarn publish dist",
    "doc:dev": "docz dev",
    "doc:build": "docz build",
    "doc:publish": "docz build && gh-pages -d .docz/dist"
  },
  "prettier": {
    "printWidth": 80,
    "useTabs": false,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always"
  },
  "eslintConfig": {
    "extends": "ts-lib"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "cross-env CI=true ts-lib-tools test",
      "eslint --fix --color",
      "git add"
    ]
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "styled-components": ">=4.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "immer": "^5.3.2",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "react-fast-compare": "^2.0.4",
    "rxjs": "^7.3.0",
    "scheduler": "^0.18.0",
    "shallowequal": "^1.1.0",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@sinoui/theme": "^1.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@testing-library/react-hooks": "^3.4.1",
    "@types/lodash": "^4.14.138",
    "@types/react": "^16.9.47",
    "@types/react-dom": "^16.9.8",
    "@types/scheduler": "^0.16.1",
    "@types/shallowequal": "^1.1.1",
    "@types/styled-components": "^5.1.2",
    "@types/theme-ui": "^0.3.6",
    "docz": "^2.3.1",
    "gh-pages": "^3.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.13",
    "prettier": "^2.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "styled-components": "^4.3.2",
    "ts-lib-tools": "^0.14.3",
    "tsconfig-paths-webpack-plugin": "^3.3.0"
  },
  "resolutions": {
    "typescript": "^4.5.2"
  }
}
